<script>
$(function(){
	try {
	
		if(OnRedirection) return;
		
		if(typeof(newsClass) == "undefined")
			newsClass = '';
		if(typeof(newsNo) == "undefined")
			newsNo = '';
		var uri = location.pathname;
		var cate = category;
		if(uri==''){

		}else if(uri=='/' || (/^\/section\//).test(uri)){
			var s = location.pathname.split("/");
			cate = s[2];
		}else if(/^\/news\//.test(uri)){
			var s = location.pathname.split("/");
			newsClass = s[3];
			newsNo = s[4];
			cate = s[2];
		}
		
		getScrNews(newsServer, cate, newsClass, newsNo);
		
	}catch(e) {
	 
	}
})
</script>

